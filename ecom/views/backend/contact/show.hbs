<main id="main" class="main">
    <div class="pagetitle">
        <h1>Dashboard</h1>
        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/admin">Home</a></li>
                <li class="breadcrumb-item"><a href="/admin/contact">Contact Us</a></li>
                <li class="breadcrumb-item active">Show</li>
            </ol>
        </nav>
    </div><!-- End Page Title -->

    <section class="section dashboard">

        <div class="card">
            <div class="card-body">
                <div class="mt-4 mb-4">
                    <h2 class="mt-5 mb-5 border border-success bg-success text-white rounded   "
                        style="text-align: center;">Contact Us Create</h5>

                </div>

                <a class="btn btn-outline-warning btn-sm float-end my-2" href="/admin/contact/create">Create</a>
                <div class="h4 pb-2 mb-4 text-danger border-bottom border-danger">
                </div>

                <!-- Dark Table -->
                <table class="table table-dark">
                    <thead>
                    </thead>
                    <tbody>
                        <tr>
                            <th>Title</th>
                            <td>:</td>
                            <td>
                                {{Contact.title}}
                            </td>
                        </tr>

                        <tr>
                            <th>Details</th>
                            <td>:</td>
                            <td>
                                {{Contact.details}}
                            </td>
                        </tr>

                        <tr>
                            <th>Image</th>
                            <td>:</td>
                            <td>
                                <img src="/{{Contact.image}}" width="200px" alt="">
                            </td>
                        </tr>
                    </tbody>
                </table>
                <!-- End Dark Table -->

            </div>
        </div>
    </section>









</main>